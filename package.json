    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.3.1",
    "css-loader": "^0.28.7",
    "d3": "^4.10.2",
    "dsv-loader": "^2.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "invariant": "^2.2.2",
    "morgan": "^1.8.2",
